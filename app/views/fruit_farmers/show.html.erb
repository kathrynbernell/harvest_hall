<div class="page-header">
  <h3>Fruit Farmer #<%= @fruit_farmer.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Fruit</dt>
      <dd><%= @fruit_farmer.fruit_id %></dd>

      <dt>Farmer</dt>
      <dd><%= @fruit_farmer.farmer_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/fruit_farmers" class="btn btn-primary">
        Back
      </a>
      <a href="/fruit_farmers/<%= @fruit_farmer.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_fruit_farmer/<%= @fruit_farmer.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
